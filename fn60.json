{
  "title": "fn60: use fn key to extend 60% keyboard easily",
  "rules": [
    {
      "description": "Map fn + esc to grave accent and tilde",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "optional": [ "any" ],
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
	      "modifiers": [ "left_shift" ]
            }
          ]
        }
      ]
    },
